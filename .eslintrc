{
  "env": {
    "node": true
  },
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module"
  },
  "rules": {
    "import/no-unresolved": [2],
    "indent": ["warn", 2],
    "no-unused-vars": 1,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "linebreak-style": 0,
    "quote-props": 0,
    "func-names": 0,
    "max-len": [
      "warn",
      {
        "code": 140,
        "ignoreComments": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true
      }
    ],
    "no-param-reassign": 0,
    "no-unused-expressions": 0,
    "arrow-body-style": 0,
    "new-cap": 0,
    "prefer-arrow-callback": 0,
    "prefer-template": 0,
    "no-mixed-operators": ["warn"],
    "no-else-return": 0,
    "no-use-before-define": ["warn"],
    "no-shadow": ["warn"],
    "curly": ["warn"],
    "object-curly-spacing": ["error", "always"],
    "keyword-spacing": ["warn"],
    "comma-dangle": [
      "warn",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "ignore"
      }
    ],
    "import/extensions": 0,
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0,
    "object-curly-newline": 0,
    "prefer-destructuring": 0,
    "class-methods-use-this": 0,
    "no-restricted-globals": 0,
    "guard-for-in": 0,
    "no-restricted-syntax": 0,
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "always",
        "asyncArrow": "always"
      }
    ]
  },
  "globals": {
    "describe": false,
    "it": false,
    "before": false,
    "beforeEach": false,
    "after": false,
    "afterEach": false
  }
}
